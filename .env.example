# Mail Configuration (SMTP)
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=your-email@example.com
MAIL_PASSWORD=your-app-password
MAIL_FROM=Booking Tour <your-email@example.com>
MAIL_SMTP_AUTH=true
MAIL_SMTP_STARTTLS_ENABLE=true
MAIL_DEBUG=false

# Mail Queue (Redis List)
MAIL_QUEUE_REDIS_KEY=mail:queue
MAIL_QUEUE_POP_TIMEOUT_MS=1000
MAIL_QUEUE_WORKER_INTERVAL_MS=200
MAIL_QUEUE_MAX_ATTEMPTS=5
MAIL_QUEUE_BASE_BACKOFF_MS=5000

# Stripe Configuration
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_SUCCESS_URL=http://localhost:8080/api/v1/payments/stripe/success?session_id={CHECKOUT_SESSION_ID}
STRIPE_CANCEL_URL=http://localhost:8080/api/v1/payments/stripe/cancel?session_id={CHECKOUT_SESSION_ID}
STRIPE_CURRENCY=usd

# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=admin
RABBITMQ_PASSWORD=admin123
RABBITMQ_EXCHANGE_EMAIL=email.exchange
RABBITMQ_QUEUE_EMAIL=email.queue
RABBITMQ_ROUTING_KEY_EMAIL=email.booking

# JMS Queue (using RabbitMQ as JMS provider)
JMS_QUEUE_EMAIL=email.booking.queue

# Booking Auto-Cancellation Configuration
BOOKING_AUTO_CANCEL_ENABLED=true
BOOKING_AUTO_CANCEL_EXPIRATION_MINUTES=15
BOOKING_AUTO_CANCEL_CHECK_INTERVAL_MS=300000
